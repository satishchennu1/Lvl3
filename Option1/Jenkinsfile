pipeline {
  agent any
    
  tools {nodejs "nodejs"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/asrilu448/Lvl3.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        
        sh 'npm install npm@latest -g'
      }
    }
     
    stage('Test') {
      steps {
        sh 'npm test'
      }
    }      
  }
}




